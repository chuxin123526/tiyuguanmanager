<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.wheel.tiyuguanmanager.user.po">
	<class name="Contract" table="CONTRACT">
		<id name="id" type="long" column="CONTRACTID">
			<generator class="native" />
		</id>
		<property name="type" type="int" column="CONTRACTTYPE" />
		<property name="content" type="string" length="255"
			column="CONTRACTCONTENT" />
	</class>
</hibernate-mapping>